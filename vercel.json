{
    "version": 2,
    "routes": [
      {
        "src": "/images/(.*)\\.jpg",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable",
          "content-type": "image/jpeg"
        },
        "dest": "/images/$1.jpg"
      },
      {
        "src": "/images/(.*)\\.png",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable",
          "content-type": "image/png"
        },
        "dest": "/images/$1.png"
      },
      {
        "src": "/favicon.ico",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable",
          "content-type": "image/x-icon"
        },
        "dest": "/favicon.ico"
      }
    ],
    "images": {
      "domains": ["punderous.com", "www.punderous.com"],
      "sizes": [640, 750, 828, 1080, 1200]
    }
  }