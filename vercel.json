{
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      }
    ],
    "headers": [
      {
        "source": "/images/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "images": {
      "domains": ["punderous.com", "www.punderous.com"],
      "sizes": [640, 750, 828, 1080, 1200]
    }
  }